{
    "getAll":["select mopedId,name,modelYear,rating,topspeed from moped"],

    "get":[
        "select mopedId,name,modelYear,rating,topspeed from moped",
        "where mopedId=?"
    ],
    "insert":[
        "insert into moped (mopedId, name, modelYear, rating, topspeed)",
        "values(?,?,?,?,?)"
    ],
